html
  include blocks/head

  body
    h1= title
    p= info

    each item in content
      case item.type
        when 'header'
          h2= item.text
        when 'parag'
          p= item.text
        when 'size_3'
          h3= item.text
        when 'table'
          ul= item.text
        when 'line'
          li= item.text
        when 'img'
          img(src= item.link alt= item.desc)
